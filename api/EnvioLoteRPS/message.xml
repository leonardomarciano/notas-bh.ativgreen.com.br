<?xml version="1.0"?>
<EnviarLoteRpsEnvio xmlns="http://www.abrasf.org.br/nfse.xsd">
    <LoteRps Id="lote" versao="1.00">
        <NumeroLote>{{transacao}}</NumeroLote>
        <CPFCNPJRemetente>
         <CNPJ>{{cnpjRemetente}}</CNPJ>
        </CPFCNPJRemetente>
        <InscricaoMunicipal>{{InscricaoMunicipal}}</InscricaoMunicipal>
        <QuantidadeRps>{{qtdRps}}</QuantidadeRps>
    <ListaRps>
    {{#rps}}
    <Rps>
        <InfRps Id="rps:1ABCDZ">
            <IdentificacaoRps>
                <Numero>{{numeroRps}}</Numero>
                <Serie>{{serieRps}}</Serie>
                <Tipo>{{tipoIdentificacaoRps}}</Tipo>
            </IdentificacaoRps>
            <DataEmissao>{{dataEmissao}}</DataEmissao>
            <NaturezaOperacao>{{NaturezaOperacao}}</NaturezaOperacao>
            <RegimeEspecialTributacao>{{tributacaoRps}}</RegimeEspecialTributacao>
            <OptanteSimplesNacional>{{OptanteSimplesNacional}}</OptanteSimplesNacional>
            <IncentivadorCultural>{{IncentivadorCultural}}</IncentivadorCultural>
            <Status>{{statusRps}}</Status>
            <Servico>
                <Valores>
                    <ValorServicos>{{valorServicos}}</ValorServicos> 
                    <ValorDeducoes>{{valorDeducoes}}</ValorDeducoes> 
                    <ValorPis>{{ValorPis}}</ValorPis>
                    <ValorCofins>{{ValorCofins}}</ValorCofins>
                    <ValorInss>{{ValorInss}}</ValorInss>
                    <ValorIr>{{ValorIr}}</ValorIr>
                    <ValorCsll>{{ValorCsll}}</ValorCsll>
                    <IssRetido>{{IssRetido}}</IssRetido>
                    <ValorIss>{{ValorIss}}</ValorIss>
                    <OutrasRetencoes>{{OutrasRetencoes}}</OutrasRetencoes>
                    <Aliquota>{{aliquota}}</Aliquota>
                    <DescontoIncondicionado>{{DescontoIncondicionado}}</DescontoIncondicionado>
                    <DescontoCondicionado>{{DescontoCondicionado}}</DescontoCondicionado>
                </Valores>
                <ItemListaServico>{{codigoServico}}</ItemListaServico>
                <CodigoTributacaoMunicipio>{{CodigoTributacaoMunicipio}} </CodigoTributacaoMunicipio>
                <Discriminacao>{{discriminacao}}</Discriminacao>
                <CodigoMunicipio>{{CodigoMunicipio}}</CodigoMunicipio>
            </Servico>
            <Prestador>
                <Cnpj>{{cnpjRemetente}}</Cnpj>
                <InscricaoMunicipal>{{InscricaoMunicipal}}</InscricaoMunicipal>
            </Prestador>
            <Tomador>
                 <IdentificacaoTomador>
                  <CpfCnpj>
                    <Cnpj>{{tomador.cnpj}}</Cnpj>
                    </CpfCnpj>
                    <InscricaoMunicipal>{{tomador.InscricaoMunicipal}}</InscricaoMunicipal>
                </IdentificacaoTomador>
                <RazaoSocial>{{tomador.razaoSocial}}</RazaoSocial>
                <Endereco>
                    <Endereco>{{tomador.endereco.tipoLogradouro}} {{tomador.endereco.logradouro}}</Endereco>
                 <Numero>{{tomador.endereco.numero}}</Numero>
                 <Complemento>{{tomador.endereco.complemento}}</Complemento>
                 <Bairro>{{tomador.endereco.bairro}}</Bairro>
                 <CodigoMunicipio>{{tomador.endereco.CodigoMunicipio}}</CodigoMunicipio>
                 <Uf>{{tomador.endereco.uf}}</Uf>
                 <Cep>{{tomador.endereco.cep}}</Cep>
                </Endereco>
            </Tomador>

        </InfRps>
        <Assinatura>{{assinatura}}</Assinatura>
    </Rps>
    {{/rps}}
    </ListaRps>
    </LoteRps>
</EnviarLoteRpsEnvio>